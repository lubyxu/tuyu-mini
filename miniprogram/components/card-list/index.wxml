<view
    class="card-list"
>
    <view class="card-title">
        <image class="logo" src="https://636c-cloud1-0gq8f3qi3903d318-1327253936.tcb.qcloud.la/app-assets/logo.png?sign=5f13ec1661c9f65044f281927d20ffa2&t=1718609672"/>
        <view class="card-title-text">福鱼文创系列</view>
        <image class="find-icon" src="../../images/find-icon.svg"  bind:tap="onFind"/>
    </view>
    <view
        style="background-image: url({{item.url}});"
        class="card-item"
        wx:for="{{cardData}}"
        wx:key="index"
        bind:tap="ocrClick"
        data-item="{{item}}"
    >
        <image
            class="nav-icon"
            src="https://636c-cloud1-0gq8f3qi3903d318-1327253936.tcb.qcloud.la/app-assets/nav.png?sign=b0f836800c217024b5595c19d0a71974&t=1718610955"
        />
        <view class="desc-box" style="color:{{item.fontColor}};left:{{item.left}};top:{{item.top}}">
            <text class="desc-title">{{item.title}}</text>
            <text class="desc-subtitle">{{item.subTitle}}</text>
            <text class="desc-subtitle2" wx:if="{{ item.subTitle2 }}">{{item.subTitle2}}</text>
        </view>
    </view>
</view>